<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting/"
     xml:base="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:wot="http://xmlns.com/wot/0.1/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:linkedin-job-posting="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting">
        <owl:imports rdf:resource="http://eulersharp.sourceforge.net/2003/03swap/countries"/>
        <owl:imports rdf:resource="http://www.w3.org/TR/skos-reference/skos-owl1-dl.rdf"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <dc:contributor>Cypher Group</dc:contributor>
        <dc:creator>Mohammad Torabi, Ali Tahvildari, Hossein Hosseinpour, Hamideh Haeri</dc:creator>
        <dc:description>An RDF/OWL schema for representing LinkedIn Job Postings</dc:description>
        <dc:title>LinkedIn Job Postings Ontology</dc:title>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#IsForCompany -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#IsForCompany">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#PersonCounts"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:comment>Relation between Person Counts and Company</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasCompany -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasCompany">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:comment>Relation from Company to Job Posting</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasCountry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasCountry">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isCountryOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://sweet.jpl.nasa.gov/2.3/humanJurisdiction.owl#Country"/>
        <rdfs:comment>Relation between Company and Country</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasIndustry -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasIndustry">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Industries"/>
        <rdfs:comment>Relation between Company and Company Industries</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasIndustryType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasIndustryType">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryTypeOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Industries"/>
        <rdfs:comment>Relation between JobPosting and JobIndustries</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasSkill -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasSkill">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSkillOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Skills"/>
        <rdfs:comment>Relation between Job Posting and Job Skills</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasSpecialities -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#hasSpecialities">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSpecialityOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Specialities"/>
        <rdfs:comment>Relation between Company and Company Specialities</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isAlocatedFor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isAlocatedFor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Salaries"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:comment>Relation between Salaries and JobPosting</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isBenefitOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isBenefitOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Benefits"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:comment>Relation from Benefit to Job Posting</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isCountryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isCountryOf">
        <rdfs:comment>Relation from Country to Company</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryOf">
        <rdfs:comment>Relation from Company Industries to Company</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryTypeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isIndustryTypeOf">
        <rdfs:comment>Relation from Job Industries to Job Posting</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSkillOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSkillOf">
        <rdfs:comment>Relation from Job Skills to Job Posting</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSpecialityOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#isSpecialityOf">
        <rdfs:comment>Relation from Company Specialities to Company</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#address -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#address">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Address of company&apos;s headquarters</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applicationType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applicationType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Type of application process (offsite, complex/simple onsite)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applicationUrl -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applicationUrl">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>URL where applications can be submitted</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applies -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#applies">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Number of applications that have been submitted</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#city -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#city">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>City of company headquarters.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#closedTime -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#closedTime">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment>Time to close job listing</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#compansationType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#compansationType">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Salaries"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Type of compensation for the job.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#companyId -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#companyId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#PersonCounts"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Identifier for the company associated with the job posting (maps to companies.csv)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#companySize -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#companySize">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Company grouping based on number of employees (1 Smallest - 7 Largest)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#currency -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#currency">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Salaries"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Currency in which the salary is provided.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#description -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#description">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Job description.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#employeeCount -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#employeeCount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#PersonCounts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Number of employees at company</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#expiary -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#expiary">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment>Expiration date or time for the job listing</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#followerCount -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#followerCount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#PersonCounts"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment>Number of company followers on LinkedIn</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#formattedExperienceLevel -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#formattedExperienceLevel">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#JobPosting"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Job experience level (entry, associate, executive, etc)</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#industryId -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#industryId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Industries"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The industry ID.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#industryName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#industryName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Industries"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Industry name</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#inferred -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#inferred">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#Benefits"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Whether the benefit was explicitly tagged or inferred through text by LinkedIn</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#jobId -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/alitahvildari/ontologies/2023/10/linkedin-job-posting#jobId">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Class>
